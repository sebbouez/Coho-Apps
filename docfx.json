{
  "build": {
    "markdownEngineName": "markdig",
    "markdownEngineProperties": {
      "markdigExtensions": [
        "abbreviations",
        "definitionlists",
        "tasklists",
        "footnotes",
        "diagrams"
      ]
    },
    "content": [
      {
        "files": [
          "**/*.md",
          "**/*.yml"
        ],
        "src": "docs",
        "exclude": ["_site/**"],
        "dest": "."
      }
    ],
    "resource": [
      {
        "files":  ["docs/images/**"],
        "exclude": ["_site/**"],
        "dest": "."
      }
    ],
    "overwrite": [
      {
        "files": ["docs/overwrite/**/*. md"],
        "exclude": ["_site/**"]
      }
    ],
    "globalMetadata": {
      "_appLogoPath": "",
      "_appTitle": "Coho Apps Documentation",
      "_appName": "Coho Apps",
      "_appFooter": "Â© 2025 Coho Apps",
      "_enableSearch": true
    },
    "dest": "_site",
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [
        "default",
      "modern"
    ],
    "postProcessors": [],
    "keepFileLink": false,
    "serve": {
      "port": 8080
    }
  }

}
